{
  "presets": ["@babel/preset-env", ["@babel/preset-react", { "runtime": "automatic" }]],
  "plugins": [
    "@babel/plugin-syntax-jsx",
    [
      "module-resolver",
      {
        "alias": {
          "src": "./src",
          "components": "./src/components",
          "pages": "./src/pages",
          "utils": "./src/utils"
        }
      }
    ]
  ]
}
